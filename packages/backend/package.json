{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"sideEffects": false,
	"scripts": {
		"start": "ultra start:all",
		"start:all": "pnpm run start:watch && pnpm run start:nodemon",
		"start:watch": "webpack --watch",
		"start:nodemon": "nodemon --delay 80ms --exec 'kill-port --port 3000,9228; node --inspect=0.0.0.0:9228 ./dist/index.js'",
		"build": "NODE_ENV=production webpack",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"ultra": {
		"concurrent": [
			"start:all"
		]
	},
	"author": "Anton Bessonov",
	"license": "MIT",
	"dependencies": {
		"@bessonovs/node-http-router": "0.0.6",
		"micro": "9.3.5-canary.3",
		"shared": "1.0.0"
	},
	"devDependencies": {
		"@types/micro": "7.3.6",
		"@types/node": "15.0.3"
	}
}
